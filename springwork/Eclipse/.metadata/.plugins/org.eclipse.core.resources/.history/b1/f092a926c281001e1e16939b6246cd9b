package study.day1113.mvc1;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class DataReadController {
	
	@GetMapping("/")
	public String home(){
		return "start";			
	}
	
	@GetMapping("/form1")
	public String form1(){
		return "form1";			
	}
	
	@GetMapping("/form2")
	public String form2(){
		return "form2";			
	}
	
	@GetMapping("/form3")
	public String form3(){
		return "form3";			
	}
	
	@GetMapping("/form4")
	public String form4(){
		return "form4";			
	}
	
}
